[*]
charset=utf-8
end_of_line=lf
insert_final_newline=false
indent_style=tab
indent_size=4

[{.babelrc,.stylelintrc,.eslintrc,*.bowerrc,*.jsb3,*.jsb2,*.json}]
indent_style=space
indent_size=2

[*.java]
indent_style=tab
tab_width=4

[{*.ddl,*.sql}]
indent_style=tab
tab_width=1

[*.conf]
indent_style=tab
indent_size=2

[*.feature]
indent_style=space
indent_size=2
